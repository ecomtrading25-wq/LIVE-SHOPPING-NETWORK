  "execution_rules": {
    "idempotency": {
      "enabled": true,
      "key_generation": "tool_name + params_hash",
      "ttl": "24_hours"
    },
    "retry_policy": {
      "max_retries": 3,
      "backoff": "exponential",
      "retry_codes": [429, 500, 502, 503, 504]
    },
    "timeout": {
      "default": "30_seconds",
      "long_running": "5_minutes"
    },
    "logging": {
      "level": "info",
      "include_request": true,
      "include_response": true,
      "mask_sensitive": true
    }
  },
  "approval_workflows": {
    "L0": {
      "approval_required": false,
      "auto_execute": true
    },
    "L1": {
      "approval_required": false,
      "guardrails": ["budget_check", "policy_check"],
      "auto_execute": true
    },
    "L2": {
      "approval_required": true,
      "approvers": ["manager", "coo_agent"],
      "timeout": "1_hour",
      "fallback": "escalate"
    },
    "L3": {
      "approval_required": true,
      "approvers": ["founder"],
      "timeout": "24_hours",
      "fallback": "deny"
    }
  }
